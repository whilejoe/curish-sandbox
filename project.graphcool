type File @model {
  contentType: String!
  createdAt: DateTime!
  id: ID! @isUnique
  name: String!
  secret: String! @isUnique
  size: Int!
  updatedAt: DateTime!
  url: String! @isUnique
}

type User @model {
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
  userName: String! @isUnique
  fullName: String!
  auth0UserId: String @isUnique
  email: String! @isUnique
  photoURL: String
  stories: [Story!]! @relation(name: "StoryAuthor")
}

type Story @model {
  createdAt: DateTime!
  id: ID! @isUnique
  updatedAt: DateTime!
  author: User @relation(name: "StoryAuthor")
  titleText: String
  titleDelta: String
  bodyMarkup: String
  bodyDelta: String
  description: String
  published: Boolean @defaultValue(value: false)
  tags: [Tag!]! @relation(name: "TagOnStory")
}

type Tag @model {
  id: ID! @isUnique
  key: String! @isUnique
  stories: [Story!]! @relation(name: "TagOnStory")
}