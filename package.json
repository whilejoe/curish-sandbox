{
  "name": "curish",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "abyss-form": "0.4.3",
    "auth0-js": "^8.10.1",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.5.1",
    "lodash": "^4.17.4",
    "material-design-icons-svg": "^1.1.2",
    "polished": "^1.8.0",
    "qs": "^6.5.1",
    "react": "^16.0.0",
    "react-apollo": "^1.4.16",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-dom": "^16.0.0",
    "react-portal": "^4.0.0-beta.2",
    "react-quill": "1.1.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "^1.0.14",
    "react-transition-group": "^2.2.1",
    "redux": "^3.7.0",
    "redux-action-buffer": "^1.1.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.10.1",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.2.1",
    "subscriptions-transport-ws": "^0.8.3"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.3.2",
    "babel-plugin-styled-components": "^1.2.1",
    "flow-bin": "^0.56.0",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.4",
    "prettier-eslint": "^8.2.0",
    "react-app-rewired": "^1.3.1",
    "react-snapshot": "^1.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "deploy": "npm run build && firebase deploy",
    "flow": "flow"
  },
  "reactSnapshot": {
    "exclude": [
      "/callback"
    ],
    "snapshotDelay": 300
  }
}
