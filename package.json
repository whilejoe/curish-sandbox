{
  "name": "curish",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "abyss-form": "0.4.3",
    "auth0-js": "^8.9.3",
    "auth0-lock": "^10.20.0",
    "draft-js": "^0.10.1",
    "firebase": "^4.3.1",
    "history": "^4.7.2",
    "jwt-decode": "^2.2.0",
    "localforage": "^1.5.0",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-app-rewire-styled-components": "^3.0.0",
    "react-dom": "^15.6.1",
    "react-quill": "1.1.0",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "react-scripts": "^1.0.13",
    "react-transition-group": "^2.2.0",
    "redux": "^3.7.0",
    "redux-action-buffer": "^1.1.0",
    "redux-devtools-extension": "^2.13.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^4.8.3",
    "redux-thunk": "^2.2.0",
    "reset-css": "^2.2.0",
    "styled-components": "^2.1.1",
    "subscriptions-transport-ws": "^0.8.3"
  },
  "devDependencies": {
    "@playlyfe/gql": "^2.3.2",
    "flow-bin": "^0.54.1",
    "node-sass-chokidar": "^0.0.3",
    "npm-run-all": "^4.1.1",
    "prettier": "^1.7.0",
    "prettier-eslint": "^8.1.0",
    "react-app-rewired": "^1.2.7",
    "react-snapshot": "^1.2.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive",
    "start-js": "react-app-rewired start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-app-rewired build && react-snapshot",
    "test": "react-app-rewired test --env=jsdom",
    "eject": "react-app-rewired eject",
    "deploy": "npm run build && firebase deploy",
    "flow": "flow"
  },
  "reactSnapshot": {
    "exclude": [
      "/callback"
    ],
    "snapshotDelay": 300
  }
}
